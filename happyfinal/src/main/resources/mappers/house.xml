<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.happyhouse.mapper.HouseMapper">

	<select id="getHouseList" parameterType="map" resultType="house">
		select * from houseinfo
		where dong = #{mydong}
	</select>
	
	<select id="getAptList" parameterType="String" resultType="house">
		select no,AptName, dealAmount as price, dong, area
		 from housedeal
		where AptName LIKE #{aptName}
	</select>
	
	
	

</mapper>